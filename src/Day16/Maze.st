CLASS Maze EXTENDS Object MODULE AocApp CLASSVARS ''
	VARS 'positions width height startPosition endPosition'

METHODS

"Loading"

load: positionData
	"Creates a 2D maze (array of arrays) with 2 element arrays at every position:
 	 containing occupance character and the and the lowest score reaching this point,
 	 initially nil (no score yet)"

	| positionRow |

	positions := #().
	positionData do: [ :positionDataRow |
		positionRow := #().
		positionDataRow do: [ :occupance |
			occupance = $S ifTrue: [
				startPosition := positionRow length @ positions length ].
			occupance = $E ifTrue: [
				endPosition := positionRow length @ positions length ].
			positionRow add: #( occupance nil ) ].
		positions add: positionRow ].

	width := positions first length.
	height := positions length.
!

"Converting"

toString
	| string |
	string := ''.

	positions do: [ :positionRow |
		positionRow do: [ :element |
			string := string , element first toString ].
		string := string , String newline ].

	^ string.
!

"Accessing"

at: point
	"Returns occupance character at point position."
	^ ( positions at: point y ) at: point x.
!

"Solving"

solve
	| reindeer |
	reindeer := Reindeer new
		position: startPosition;
		direction: 1 @ 0;
		score: 0.

	self solve: reindeer.

	^ ( self at: endPosition ) last.
!
solve: reindeer
	| element occupance score newReindeer |

	element := self at: reindeer position.

	"Occupance is wall."
	occupance := element first.
	occupance = $# ifTrue: [ ^ self ].

	"Score of a previous is already lower or equal to that of current path.
	 This also catches a reindeer crossing its own path."
	score := element last.
	( score notNil and: [ score <= reindeer score ] ) ifTrue: [ ^ self ].
	element at: 1 put: reindeer score.

	"Reached end position."
	( reindeer position = endPosition ) ifTrue: [ ^ self ].

	newReindeer := self solve: reindeer copy advance.
	newReindeer := self solve: reindeer copy turnLeft advance.
	newReindeer := self solve: reindeer copy turnRight advance.
!

